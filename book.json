{
    "title": "数值代数课件",
    "description": "这是魏华祎的数值代数课程的课件",
    "author": "Wei, Huayi",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "plugins": [
        "-highlight", 
        "prism", 
        "prism-themes",
        "mathjax", 
        "image-captions", 
        "fontsettings", 
        "expandable-chapters-small", 
